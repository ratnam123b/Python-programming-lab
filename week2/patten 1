import random
y = int(input("number of gusses:"))
n=random.randint(1,100)
while (y>=1):
  
    x = int(input("guess the number:"))
    if(x<n):
    	print("the number is too low")
    elif(x>n):
        print("the number is too high")
    else:
        print("your guess is correct")
    y=y-1
    
if(y==0):
       print("you lost the game")
